{
    "name": "typescript-express",
    "version": "1.0.0",
    "description": "Express project template (Type Script)",
    "main": "index.js",
    "scripts": {
        "format": "prettier --write --tab-width 4 \"**/*.ts\"",
        "start": "ts-node -r tsconfig-paths/register src/index.ts",
        "dev": "nodemon",
        "prestart:prod": "rm -rf dist && tsc",
        "start:prod": "node dist/index.js",
        "test": "mocha test/**/*.ts --exit",
        "webpack": "webpack --config webpack.config.js"
    },
    "dependencies": {
        "@types/cookie-parser": "^1.4.1",
        "@types/dotenv": "^4.0.3",
        "body-parser": "^1.18.2",
        "cookie-parser": "^1.4.3",
        "dotenv": "^4.0.0",
        "express": "^4.16.2",
        "pino": "^4.7.3",
        "source-map-support": "^0.4.15",
        "swagger-express-middleware": "^1.0.0-alpha.12",
        "typescript": "^2.6.2"
    },
    "devDependencies": {
        "@types/chai": "^4.1.2",
        "@types/express": "^4.0.35",
        "@types/mocha": "^2.2.48",
        "@types/node": "^9.3.0",
        "@types/pino": "^4.7.1",
        "@types/supertest": "^2.0.4",
        "@types/swagger-express-middleware": "^1.0.8",
        "@types/webpack-node-externals": "^1.6.3",
        "chai": "^4.1.2",
        "copy-webpack-plugin": "^4.5.1",
        "mocha": "^4.0.1",
        "nodemon": "^1.14.1",
        "prettier": "^1.11.1",
        "supertest": "^3.0.0",
        "ts-jest": "^21.2.4",
        "ts-loader": "^4.1.0",
        "ts-node": "^4.1.0",
        "tsconfig-paths": "^3.1.1",
        "tslint": "5.3.2",
        "webpack": "^4.2.0",
        "webpack-cli": "^2.0.13",
        "webpack-node-externals": "^1.6.0"
    }
}
